.ljit-lottery-list
	width: 288px
	height: 100%
	overflow-y: scroll
	background: #fff
	padding: 6px 0
	border-radius: 4px

	.data-inline-list__list-item
		cursor: pointer
		&:hover
			background: #fbfbfb

	.ljit-list-item__wrapper
		.ljit-lottery-icon
			margin-right: 10px

	&--hide-end-time
		.ljit-countdown
			display: none
